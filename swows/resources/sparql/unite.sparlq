PREFIX df:   <http://www.swows.org/dataflow#>
PREFIX swi:   <http://www.swows.org/instance#>

CONSTRUCT
    ?s ?p ?o .
    ?constructGraph ?pR ?oR .
    ?sG ?pG ?oG .
  WHERE
    ?s ?p ?o .
    ?constructGraph
      a df:ConstructGraph ;
      df:config ?importedGraph .
    ?importedGraph
      a df:ImportedGraph ;
      df:uri ?graphName .
    GRAPH ?graphName {
      ?sG ?pG ?oG .
      FILTER (?sG != swi:GraphRoot) .
      swi:GraphRoot ?pR ?oR .
    }